healthyness <- function(df){
for(i in 0:nrow(df)){
df$Healthy[i] <- TRUE;
if (df$HDL[i] < 40){
df$HDL.LEVEL[i] <- "Baixo"
df$Healthy[i] <- FALSE;
}
else if (df$HDL[i] < 60){
df$HDL.LEVEL[i] <- "Medio"
}
else if (df$HDL[i] >= 60){
df$HDL.LEVEL[i] <- "Desejável"
}
if (df$Colesterol[i] < 200){
df$ColesterolFaixa[i] <- "Desejável"
}
else if (df$Colesterol[i] <= 239){
df$ColesterolFaixa[i] <- "Limítrofe"
}
else if (df$Colesterol[i] > 239){
df$ColesterolFaixa[i] <- "Alto"
df$Healthy[i] <- FALSE;
}
if (((df$Sexo[i] == "female" && df$HDL[i] < 50)|| (df$Sexo[i] == "male" && df$HDL[i] < 40))  && df$Pressao.Sistolica[i] >= 130 && df$Pressao.Diast[i] >= 85 && df$Glicose >= 100){
df$Displazia[i] <- TRUE
df$Healthy[i] <- FALSE;
}
else{
df$Displazia[i] <- FALSE
}
}
}
